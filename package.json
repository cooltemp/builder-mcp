{
  "name": "builder-mcp",
  "version": "1.0.0",
  "description": "Simple Builder.io MCP for headless CMS integration with Qwik",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "dev:mcp": "tsx watch src/mcp/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/app.js",
    "start:mcp": "node dist/mcp/server.js",
    "type-check": "tsc --noEmit",
    "generate-types": "tsx src/scripts/generateTypes.ts",
    "debug-models": "tsx src/scripts/debugModels.ts",
    "validate-types": "tsx src/scripts/validateTypes.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:models": "vitest run tests/models.test.ts",
    "test:content": "vitest run tests/content.test.ts",
    "test:upload": "vitest run tests/upload.test.ts",
    "test:types": "vitest run tests/types.test.ts",
    "test:all": "vitest run tests/"
  },
  "keywords": [
    "builder.io",
    "mcp",
    "cms",
    "qwik",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.4",
    "@vitest/ui": "^1.0.4",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  }
}
